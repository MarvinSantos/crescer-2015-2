@model Locadora.Web.MVC.ViewModels.JogoLocacaoModel
@{
    ViewBag.Title = "Locacao";
}

<h2>Locacao</h2>

@Html.Partial("_JogoLocacao",Model)


@using (Html.BeginForm("SalvarLocacao", "Jogo", FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
{

    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

    <div class="form-group">
        @Html.LabelFor(m => m.Cliente.Nome, new { @class = "col-sm-2 control-label" })
        <div class="col-sm-10">
            @Html.TextBoxFor(m => m.Cliente.Nome, new { @class = "form-control" })
            @Html.ValidationMessageFor(m => m.Cliente.Nome)
        </div>
    </div>

    <input type="submit" name="enviar" value="jogo" />

}

